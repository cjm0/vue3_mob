{
  "name": "vue3",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "build": "run-p vite build --mode test",
    "test": "run-p type-check vite build --mode test",
    "pre": "run-p type-check vite build --mode pre",
    "prod": "run-p type-check vite build --mode prod",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "prepare": "husky install",
    "format": "prettier --write src/",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx}": [

    ]
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "fastclick": "^1.0.6",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "start-server-and-test": "^2.0.0",
    "terser": "^5.16.9",
    "typescript": "~4.8.4",
    "vconsole": "^3.15.0",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vconsole": "^1.3.1",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
