{
  "name": "vue3",
  "version": "1.1.0",
  "description": "Vue3 + Vite5 Mobie Template",
  "type": "module",
  "scripts": {
    "dev": "vite dev --mode dev",
    "build": "vite build --mode prod",
    "test": "run-p type-check && vite build --mode test",
    "pre": "run-p type-check && vite build --mode pre",
    "prod": "run-p type-check && vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "preview": "vite preview",
    "transform": "vite --debug transform",
    "plugin-transform": "vite --debug plugin-transform",
    "hmr": "vite --debug hmr",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "prepare": "husky",
    "format": "prettier --write src/",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^11.0.3",
    "axios": "^1.7.2",
    "fastclick": "^1.0.6",
    "pinia": "^2.2.0",
    "vue": "^3.4.34",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "start-server-and-test": "^2.0.4",
    "terser": "^5.31.1",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "vconsole": "^3.15.1",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-restart": "^0.4.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  },
  "private": false,
  "license": "MIT",
  "author": {
    "name": "前端一锅煮",
    "email": "1293004002@qq.com"
  },
  "homepage": "https://github.com/cjm0/vue3_mob",
  "repository": {
    "type": "git",
    "url": "git@github.com:cjm0/vue3_mob.git"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  }
}
